{
  "expo": {
    "name": "react-native-boilerplate",
    "slug": "react-native-boilerplate",
    "version": "3.0.0",
    "owner": "wataru",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#f5f5f5", 
      "dark": {
        "backgroundColor": "#101212"
      },
      "image": "./assets/images/logo-lg.png", 
      "resizeMode": "cover", 
      "fade": true  
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wataru.react-native-boilerplate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-lg.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/logo-lg.png"
    },
    "scheme": "myapp"
  }
}
